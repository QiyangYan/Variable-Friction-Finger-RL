<mujoco>
    <size njmax="500" nconmax="100" nuser_jnt="1" nuser_site="1" nuser_sensor="1" nuser_actuator="5" nstack="600000"></size>
    
    <visual>
        <map fogstart="3" fogend="5" force="0.1"></map>
        <quality shadowsize="4096"></quality>
    </visual>
    
    <contact>
        <exclude body1="base_link" body2="left_finger"></exclude>
        <exclude body1="base_link" body2="right_finger"></exclude>
        <exclude body1="left_finger" body2="left_insert"></exclude>
        <exclude body1="right_finger" body2="right_insert"></exclude>
    </contact>

    <default>
        <default class="visual">
            <geom type="mesh" contype="0" conaffinity="0" group="2"></geom>
        </default>
        <default class="collision">
            <geom type="mesh" group="3"></geom>
            <default class="left_insert_box">
                <geom mass="0" type="box" pos="0.012 -0.051365 0.0113936"
                size="0.0121 0.053 0.010375" friction="0.1" solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" rgba="0.9 0.3 0.55 1"></geom>
            </default>
            <default class="right_insert_box">
                <geom mass="0" type="box" pos="0.012 -0.051365 0.0287911" size="0.0121 0.053 0.010375" friction="0.1" solimp="0.95 0.99 0.001" solref="0.004 1" priority="1" rgba="0.9 0.3 0.55 1"></geom>
            </default>
            <default class="left_finger_box">
                <geom type="box" size = "0.016 0.06 0.003" contype="1" conaffinity="1" group="1" density="0" rgba="0.96 0.89 1.07 0.7" solimp="0.95 0.99 0.001" solref="0.00001 1" friction = "1" pos="-0.00218 0.087 0.019" priority="1"></geom>
            </default>
            <default class="right_finger_box">
                <geom type="box" size = "0.016 0.06 0.003" contype="1" conaffinity="1" group="1" density="0" rgba="0.96 0.89 1.07 0.7" solimp="0.95 0.99 0.001" solref="0.00001 1" friction = "1"
                      pos="0.00218 0.087 0.019" priority="1"></geom>
            </default>
        </default>
    </default>
  
    <sensor>
        <jointpos name="joint:p_left" joint="joint:left"></jointpos>
        <jointpos name="joint:p_right" joint="joint:right"></jointpos>
        <jointpos name="joint:p_leftInsert" joint="joint:leftInsert"></jointpos>
        <jointpos name="joint:p_rightInsert" joint="joint:rightInsert"></jointpos>
        <jointpos name="joint:p_stand" joint="joint:stand"></jointpos>
    </sensor>
  
    <actuator>
        <position name="joint:A_left" joint="joint:left" ctrlrange="-1.0472 1.0472" kp="10" forcerange="-4.785 4.785"></position>
        <position name="joint:A_right" joint="joint:right" ctrlrange="-1.0472 1.0472" kp="10" forcerange="-4.785 4.785"></position>
        <position name="joint:A_leftInsert" joint="joint:leftInsert" ctrlrange="0 0.34" gear = "100" kp="20"></position>
        <position name="joint:A_rightInsert" joint="joint:rightInsert" ctrlrange="0 0.34" gear = "100" kp="20" ></position>
        <position name="joint:A_stand" joint="joint:stand" ctrlrange="0 3" kp="20"></position>
    </actuator>
</mujoco>

